{
  "name": "aha-flow-app",
  "version": "1.0.0",
  "description": "GitHub App for StanfordAHA CI flow",
  "author": "Keyi Zhang <keyi@cs.stanford.edu> (keyizhang.com)",
  "license": "ISC",
  "repository": "https://github.com//aha-flow-app.git",
  "homepage": "https://github.com//aha-flow-app",
  "bugs": "https://github.com//aha-flow-app/issues",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "dev": "nodemon",
    "start": "probot run ./index.js",
    "lint": "standard --fix",
    "test": "jest && standard",
    "test:watch": "jest --watch --notify --notifyMode=change --coverage"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "probot": "^12.2.8",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.8"
  },
  "devDependencies": {
    "jest": "^24.9.0",
    "nock": "^10.0.0",
    "nodemon": "^1.19.4",
    "smee-client": "^1.0.2",
    "standard": "^12.0.1"
  },
  "engines": {
    "node": ">= 8.3.0"
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "nodemonConfig": {
    "exec": "npm start",
    "watch": [
      ".env",
      "."
    ]
  },
  "jest": {
    "testEnvironment": "node"
  }
}
